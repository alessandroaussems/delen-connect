<!DOCTYPE html>

<title>Customer</title>

{% block body %}
    <h1>Customer</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Created</th>
                <td>{{ customer.created ? customer.created|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Updated</th>
                <td>{{ customer.updated ? customer.updated|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Id</th>
                <td>{{ customer.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ customer.name }}</td>
            </tr>
            <tr>
                <th>LastName</th>
                <td>{{ customer.lastName }}</td>
            </tr>
            <tr>
                <th>YearOfBirth</th>
                <td>{{ customer.yearOfBirth ? customer.yearOfBirth|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>YearOfDeath</th>
                <td>{{ customer.yearOfDeath ? customer.yearOfDeath|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>City</th>
                <td>{{ customer.city }}</td>
            </tr>
            <tr>
                <th>Country</th>
                <td>{{ customer.country }}</td>
            </tr>
            <tr>
                <th>Sex</th>
                <td>{{ customer.sex }}</td>
            </tr>
            <tr>
                <th>Language</th>
                <td>{{ customer.language }}</td>
            </tr>
            <tr>
                <th>Type</th>
                <td>{{ customer.type }}</td>
            </tr>
            <tr>
                <th>StatRv</th>
                <td>{{ customer.statRv }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('customer_index') }}">back to list</a>

    <a href="{{ path('customer_edit', {'id': customer.id}) }}">edit</a>

    {{ include('customer/_delete_form.html.twig') }}
{% endblock %}
