<form method="POST" style="top: 80vh" id="satisfactionform" onsubmit="addSatisfaction()">
    <label for="myRange">Wat vond u van uw laatste bezoek?</label>
    <div class="info">
        <span>Niet tevreden</span>
        <span>Zéér tevreden</span>
    </div>
    <div class="slidecontainer">
        <input type="range" min="1" max="100" value="60" class="slider" id="myRange" name="range">
    </div>
    <button>Toevoegen</button>
</form>
