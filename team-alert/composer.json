{
  "name": "alert/alert",
  "type": "project",
  "license": "proprietary",
  "require": {
    "php": "^7.1.3",
    "ext-iconv": "*",
    "a2lix/translation-form-bundle": "^2.4",
    "doctrine/doctrine-fixtures-bundle": "^3.0",
    "doctrine/doctrine-migrations-bundle": "^1.3",
    "friendsofsymfony/ckeditor-bundle": "^2.0",
    "helios-ag/fm-elfinder-bundle": "^8.0",
    "isometriks/spam-bundle": "^1.0",
    "knplabs/doctrine-behaviors": "^1.5",
    "php-translation/symfony-bundle": "^0.7.0",
    "slot/mandrill-bundle": "^1.1",
    "sonata-project/admin-bundle": "^3.40.2",
    "sonata-project/core-bundle": "3.10.0",
    "sonata-project/datagrid-bundle": "^2.3",
    "sonata-project/doctrine-orm-admin-bundle": "^3.5",
    "symfony/apache-pack": "^1.0",
    "symfony/asset": "^3.4",
    "symfony/console": "^3.4",
    "symfony/dotenv": "^3.4",
    "symfony/expression-language": "^3.4",
    "symfony/flex": "^1.1",
    "symfony/form": "^3.4",
    "symfony/maker-bundle": "^1.7",
    "symfony/monolog-bundle": "^3.1",
    "symfony/orm-pack": "*",
    "symfony/process": "^3.4",
    "symfony/security-bundle": "^3.4",
    "symfony/serializer-pack": "*",
    "symfony/twig-bundle": "^3.4",
    "symfony/validator": "^3.4",
    "symfony/web-link": "^3.4",
    "symfony/web-profiler-bundle": "^3.4",
    "symfony/webpack-encore-pack": "*",
    "symfony/yaml": "^3.4"
  },
  "require-dev": {
    "symfony/browser-kit": "^3.4",
    "symfony/css-selector": "^3.4",
    "symfony/phpunit-bridge": "^3.4",
    "symfony/profiler-pack": "*",
    "symfony/var-dumper": "^4.2",
    "symfony/web-server-bundle": "^3.4"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true,
    "component-dir": "public/assets",
    "platform": {
      "php": "7.1.8"
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php70": "*",
    "symfony/polyfill-php56": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false
    }
  }
}
